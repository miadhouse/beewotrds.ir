{
    "name": "miad/beewords-api",
    "description": "API for Beewords application",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Miad\\BeewordsApi\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Miad Aleali",
            "email": "miad.aleali@nera-ai.de"
        }
    ],
    "require": {
        "php": ">=8.0",
        "firebase/php-jwt": "^6.0",
        "symfony/http-foundation": "^6.0",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "start": "php -S localhost:8000 -t ",
        "test": "../vendor/bin/phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
